{{#if addNewListing}}
  <h1>New Listings</h1>
  <div>
    <form>
      <div class="form-group">
        <label for="name">Listing Title</label>
        {{input value=name id="name"}}
      </div>
      <div class="form-group">
        <label for="timestamp">Date Added</label>
        {{input value=timestamp id="timestamp"}}
      </div>
      <div class="form-group">
        <label for="money">Money</label>
        {{input value=money id="money"}}
      </div>
      <div class="form-group">
        <label for="type">type</label>
        {{input value=type id="type"}}
      </div>
      <button {{action 'saveListing1'}}>Save</button>
    </form>
  </div>
{{else}}
  <button {{action 'listingFormShow'}}>New Listing</button>
{{/if}}
